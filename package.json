{
  "name": "attakk-store",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "build:products": "tsx -r tsconfig-paths/register --env-file=.env ./scripts/create-products.ts",
    "preview": "astro preview",
    "astro": "astro",
    "code": "npm run format && npm run lint",
    "code:fix": "npm run format:fix && npm run lint:fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix"
  },
  "dependencies": {
    "@astrojs/node": "^9.4.4",
    "@astrojs/preact": "^4.1.1",
    "@fontsource-variable/urbanist": "^5.2.6",
    "@fontsource/permanent-marker": "^5.2.6",
    "@nanostores/persistent": "^1.1.0",
    "@nanostores/preact": "^1.0.0",
    "@stripe/stripe-js": "^7.8.0",
    "@tailwindcss/vite": "^4.1.8",
    "astro": "^5.14.1",
    "nanostores": "^1.0.1",
    "preact": "^10.26.8",
    "stripe": "^18.4.0",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/stripe": "^8.0.416",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript-eslint": "^8.33.1"
  }
}
