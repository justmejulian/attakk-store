---
import logo from '@images/attakk_studios_black.svg';
import CartButton from '@components/CartButton';

interface Props {
  title: string;
}
const { title } = Astro.props;
---

<nav
  class="mx-auto mb-4 flex h-16 max-w-screen-xl items-center justify-between"
>
  <a href="/" class="flex items-center">
    <img src={logo.src} alt="Attakk Studios Logo" class="h-8" />
  </a>

  <CartButton client:load />
</nav>
